@model IEnumerable<OdeToFood.Data.Model.Restaurant>

@{
    ViewBag.Title = "Main Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <div>
        There are @Model.Count() restaurant in the db.
    </div>
</div>
<ul>
    @foreach (var restaurant in Model)
    {
       <li>@restaurant.Name</li>
    }
</ul>
